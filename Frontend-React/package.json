{
  "name": "todolist.app",
  "version": "0.1.0",
  "private": true,
  "author": "ClearPoint",
  "dependencies": {
    "axios": "^1.2.2",
    "bootstrap": "^5.2.3",
    "cross-env": "^7.0.3",
    "node-sass": "^8.0.0",
    "@playwright/test": "^1.39.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^3.1.1"
  },
  "scripts": {
    "start": "cross-env BROWSER=chrome react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test:run-docker": "docker run -it --ipc=host --user $(id -u) -v $(pwd):/app -p 9323:9323 mcr.microsoft.com/playwright:v1.39.0-jammy /bin/bash",
    "test": "playwright test --config ./playwright.config.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^2.8.3"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@axios)/"
    ]
  }
}
